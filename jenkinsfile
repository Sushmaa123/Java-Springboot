pipeline {
    agent any
    stages {
        stage('git checkout') {
            steps {
                git branch: 'feature', url: 'https://github.com/Sushmaa123/Java-Springboot.git'
            }
        }
        stage('build') {
            steps {
                bat 'mvn clean install'
            }
        }
        stage('test') {
            steps {
                echo 'this is test phase'
            }
        }
        stage('deploy') {
            steps {
                echo 'this is deploy phase'
            }
        }
    }
}
